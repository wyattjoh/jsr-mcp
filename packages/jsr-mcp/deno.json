{
  "$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
  "name": "@wyattjoh/jsr-mcp",
  "version": "1.2.0",
  "license": "MIT",
  "exports": "./mod.ts",
  "publish": {
    "include": ["LICENSE", "README.md", "mod.ts", "src/**/*.ts", "deno.json"]
  },
  "tasks": {
    "build": "deno compile --allow-env=JSR_API_URL,JSR_REGISTRY_URL,JSR_API_TOKEN --allow-net=api.jsr.io,jsr.io --output=jsr-mcp mod.ts"
  },
  "imports": {
    "@modelcontextprotocol/sdk": "npm:@modelcontextprotocol/sdk@^1.25.2",
    "zod": "npm:zod@^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wyattjoh/jsr-mcp.git"
  }
}
